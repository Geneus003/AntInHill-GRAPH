import graph
import perebor
import time


def main_func(Qu):
  rezult = {}
  
  for i in range(Qu):
    matrix, n = graph.generate_graph()
    time_ = time.time()
    len_, road = perebor.mainus(matrix, n)
    time_ = time.time() - time_
    rezult[i] = [matrix, n, len_, time_]
  return rezult


if __name__ == "__main__":
  f = open('graph_with_len.txt', 'w')
  f.write(main_func(500))
